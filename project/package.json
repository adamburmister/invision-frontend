{
  "name": "invision-frontend",
  "version": "0.0.1",
  "description": "InVision frontend project",
  "main": "dist/index.html",
  "homepage": "https://github.com/adamburmister/invision-frontend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamburmister/invision-frontend.git"
  },
  "bugs": "https://github.com/adamburmister/invision-frontend/issues",
  "author": {
    "name": "Adam Burmister",
    "email": "adam.burmister@gmail.com",
    "url": "http://burmister.com"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "buildjs": "jspm bundle-sfx js/app dist/app.min.js --minify --skip-source-maps",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss"
  },
  "keywords": [],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "del": "^1.2.0",
    "esdoc": "^0.1.2",
    "eslint": "^0.22.1",
    "eslint-config-standard": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^0.13.2",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sass": "^2.0.1",
    "gulp-scss-lint": "^0.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "imagemin-pngquant": "^4.1.0",
    "karma": "^0.12.36",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jspm": "^1.1.5",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-sinon-chai": "^1.0.0",
    "mocha": "^2.2.5",
    "node-bourbon": "^4.2.3",
    "node-neat": "^1.7.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "sassdoc": "^2.1.15"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "isotope-fit-columns": "npm:isotope-fit-columns@^1.1.2",
      "isotope-layout": "npm:isotope-layout@^2.2.2",
      "react": "npm:react@~0.13.3",
      "react-dom": "npm:react-dom@^0.13.3",
      "react-redux": "npm:react-redux@^0.9.0",
      "react-router": "npm:react-router@^0.13.3",
      "redux": "npm:redux^1.0.1",
      "redux-logger": "npm:redux-logger@^1.0.5",
      "redux-thunk": "npm:redux-thunk@^0.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  },
  "dependencies": {
    "node-neat": "^1.7.2"
  }
